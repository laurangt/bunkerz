<h1 class="m-auto">Bunkers in Innsbruck</h1>
  <% @bunkers.each do |bunker| %>
<div class="cards" style="margin: auto;">
  <div class="card mb-3 p-2 d-flex" style="height: 100%; width: 50vw; text-align: center">
      <% if bunker.photo.key.present? %>
        <%= cl_image_tag bunker.photo.key, height: 100, width: 100, crop: :fill %>
      <% end %>
      <h2><%= bunker.title %></h2>
      <%= bunker.summary %>
      <p><%= bunker.price %>â‚¬</p>
      <p><%= link_to "Delete bunker", bunker_path(bunker), data: {turbo_method: :delete, turbo_confirm: "Are you sure?"}, class: "text-danger" %></p>
  </div>
    <% end %>
