<%= link_to profile_path do %>
  <i class="fa-solid fa-arrow-left"></i>
<% end %>

<h1>Add a new bunker</h1>

<%= simple_form_for (@bunker) do |f| %>
  <%= f.input :title %>
  <%= f.input :location %>
  <%= f.input :description %>
  <%= f.input :price %>
  <%= f.input :feature %>
  <%= f.input :summary %>
  <%= f.input :photo, as: :file %>
  <%#= f.input :photos, as: :file, input_html: { multiple: true } %>
  <%= f.button :submit %>
<% end %>
